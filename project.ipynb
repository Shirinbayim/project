   "outputs": [],
   "source": [
    "# Calculate mean historical return and covariance matrix\n",
    "mu = expected_returns.mean_historical_return(df_stocks)\n",
    "Sigma = risk_models.sample_cov(df_stocks)"
   ]
  },
  {
   "cell_type": "code",
